    "sharp": "^0.34.5"
  },
  "scripts": {
    "dev": "electron-forge start",
    "build": "vite build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "deploy": "electron-forge make --publish always",
